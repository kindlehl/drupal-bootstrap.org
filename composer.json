{
    "require": {
        "php": ">=5.3.0",
        "sensiolabs/security-checker": "~2.0",
        "erusev/parsedown-extra": "0.7.*",
        "gitonomy/gitlib": "~0.1.7"
    },
    "config": {
        "autoloader-suffix": "ComposerManager",
        "vendor-dir": "docroot/../vendor"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Drupal\\DBMarkdown\\": [
                "docroot/sites/all/modules/custom/db_markdown/src"
            ],
            "Drupal\\GitClone\\": [
                "docroot/sites/all/modules/contrib/git_clone/src"
            ]
        }
    }
}