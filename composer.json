{
  "repositories": [
      {
          "type": "vcs",
          "url": "https://github.com/jdavidbakr/commonmark-attributes-extension"
      }
  ],
    "require": {
        "league/commonmark": "~0.17.0",
        "php": ">=5.6.5",
        "webuni/commonmark-attributes-extension": "dev-master",
        "webuni/commonmark-table-extension": "~0.8.0",
        "sensiolabs/security-checker": "~2.0",
        "gitonomy/gitlib": "~0.1.7"
    },
    "config": {
        "autoloader-suffix": "ComposerManager",
        "vendor-dir": "docroot/../vendor"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Drupal\\CommonMark\\": [
                "docroot/sites/all/modules/contrib/commonmark/src"
            ],
            "Drupal\\GitClone\\": [
                "docroot/sites/all/modules/contrib/git_clone/src"
            ]
        }
    }
}
